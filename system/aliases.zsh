alias ls='grc --colour=auto ls --color=always'  # !See .oh-my-zsh/oh-my-zsh.sh overrides
alias pbcopy=’xclip -selection clipboard’
alias pbpaste=’xclip -selection clipboard -o’
alias wshutdown='cmd.exe /c "wsl --shutdown"'
alias dotfiles='/usr/bin/git --git-dir=$HOME/.chrisdotfiles/ --work-tree=$HOME'
